# This tmux statusbar config was created by tmuxline.vim
# on Sun, 15 May 2022

set -g status-justify "left"
set -g status "on"
set -g status-left-style "none"
set -g message-command-style "fg=#4f3829,bg=#eee0b7"
set -g status-right-style "none"
set -g pane-active-border-style "fg=#7c6f64"
set -g status-style "none,bg=#f5edca"
set -g message-style "fg=#4f3829,bg=#eee0b7"
set -g pane-border-style "fg=#eee0b7"
set -g status-right-length "100"
set -g status-left-length "100"
setw -g window-status-activity-style "none"
setw -g window-status-separator ""
setw -g window-status-style "none,fg=#4f3829,bg=#f5edca"
set -g status-left "#[fg=#f9f5d7,bg=#7c6f64,bold] #S #[fg=#7c6f64,bg=#f5edca,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=#eee0b7,bg=#f5edca,nobold,nounderscore,noitalics]#[fg=#4f3829,bg=#eee0b7] %Y-%m-%d  %H:%M #[fg=#7c6f64,bg=#eee0b7,nobold,nounderscore,noitalics]#[fg=#f9f5d7,bg=#7c6f64,bold] #h "
setw -g window-status-format "#[fg=#4f3829,bg=#f5edca] #I #[fg=#4f3829,bg=#f5edca] #W "
setw -g window-status-current-format "#[fg=#f5edca,bg=#eee0b7,nobold,nounderscore,noitalics]#[fg=#4f3829,bg=#eee0b7] #I #[fg=#4f3829,bg=#eee0b7] #W #[fg=#eee0b7,bg=#f5edca,nobold,nounderscore,noitalics]"
